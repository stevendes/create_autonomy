<?xml version='1.0'?>
<sdf version='1.6'>
  <model name='toll'>
   <static>1</static>
   <link name='toll_base'>
    <visual name='visual'>
      <geometry>
        <box>
          <size>0.1 0.1 0.2</size>
        </box>
      </geometry>
    </visual>
   </link>

  <joint name="toll_joint" type="revolute">
     <parent>toll_base</parent>
     <child>toll_mobile</child>
     <pose frame=''>0 0 0.3 0 0 0</pose>

     <axis>
      <xyz>0 1 0 </xyz>
    </axis>
    <limit>
      <lower>0</lower>
      <upper>1.57</upper>
      <velocity>5</velocity>
    </limit>

<!--    <gazebo>
      <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
        <namespace>New_Joint</namespace>
      </plugin>
    <gazebo> -->


   </joint>

   <link name='toll_mobile'>
    <pose frame='toll_base'>0.2 0 0 0 0 0</pose>
    <visual name='visual'>
      <geometry>
        <box>
          <size>0.5 0.025 0.07</size>
        </box>
      </geometry>
    </visual>
  <collision name='toll_collision'>
    <pose frame='toll_base'>0.2 0 0 0 0 0</pose>
      <geometry>
        <box>
          <size>0.5 0.025 0.07</size>
        </box>
      </geometry>
    </collision>
    
    
   </link>


  </model>

