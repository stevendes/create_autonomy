<?xml version='1.0'?>
<sdf version='1.6'>
  <model name='toll'>
<!--   <static>1</static> -->
   <link name='toll_base'>
    <visual name='visual'>
      <geometry>
        <box>
          <size>0.1 0.1 0.2</size>
        </box>
      </geometry>
    </visual>
   </link>

  <joint name="toll_joint" type="revolute">
     <parent>toll_base</parent>
     <child>toll_mobile</child>
     <pose frame=''>-0.25 0 0 0 0 0</pose>
     <axis>
      <xyz>0 1 0 </xyz>
      <limit>
        <lower>0</lower>
        <upper>1.57</upper>
        <velocity>1</velocity>
        <effort>5</effort>
      </limit>
      </axis>
    </joint>

   <link name='toll_mobile'>
    <pose frame='toll_base'>0.3 0.0 0 3.14 0 0</pose>
    <visual name='visual'>
      <geometry>
        <box>
          <size>0.5 0.025 0.07</size>
        </box>
      </geometry>
      <material>
        <script>
          <uri>model://create_autorace/toll/materials/scripts</uri>
          <name>warning25</name>
        </script>  
      </material>
    </visual>
  <collision name='toll_collision'>
      <geometry>
        <box>
          <size>0.5 0.025 0.07</size>
        </box>
      </geometry>
    </collision>
   </link>

     <joint name="fixed_base" type="revolute">
     <pose frame=''>0 0 -0.1 0 0 0</pose>
     <parent>world</parent>
     <child>toll_base</child>
     <axis>
        <xyz>0 0 1 </xyz>
        <limit>
          <lower>0.0000</lower>
          <upper>0.0000"</upper>
        </limit>
     </axis>
    </joint>

 <plugin name="toll_plugin" filename="libtoll_plugin.so"/>
  </model>
 
  </sdf>
